{
  "buildCommand": "pnpm run build",
  "outputDirectory": "./docs/.vitepress/dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true
}
